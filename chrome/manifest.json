{
  "manifest_version": 2,

  "name": "goflix",
  "description": "A tool to disable Netflix's 'Continue Watching' function",
  "version": "1.0",

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Toggle goflix"
  },
  "permissions": [
    "storage",
    "tabs",
    "https://*/*",
    "http://*/*"
  ]
}
